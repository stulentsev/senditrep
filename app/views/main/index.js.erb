var parameters = <%= raw params.to_json %>;
var message = check_parameters(parameters);

if (message)
	{
		$('#notification').css("display","block");
		$('#notification').html(message);
	}
else
	{
		//hide all warning notifications
		$('#notification').css("display","none");
		//render result table
		<%=render :partial =>"table.js.erb"%>	
	}
