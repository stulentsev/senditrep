var message = '';
<% if params[:from].blank? %>	
	message += ' город отправления,';
<% end %>
<% if params[:to].blank? %>
	message += ' город доставки,';
<% end %>
<% if params[:weight].blank? %>
	message += ' вес посылки или габариты посылки.';
<% end %>
if (message!='')
	{
	message = "Заполните поля:"+message;
	message = message.substring(0,message.length-1);
	$('#notification').css("display","block");
	$('#notification').html(message);
	}
else
	{
		<%=render :partial =>"table.js.erb"%>	
	}
